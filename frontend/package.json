{
  "name": "grove-fe",
  "version": "1.0.0",
  "description": "grove frontend",
  "main": "index.js",
  "scripts": {
    "watch": "npm-run-all --parallel watch:*",
    "watch:windi": "windicss ../templates/**/*.jet ../templates/*.jet -f ./windi.config.ts -do build/main.css",
    "build": "npm-run-all --parallel build:*",
    "build:windi": "windicss ../templates/**/*.jet ../templates/*.jet -f ./windi.config.ts -cmo build/main.css",
    "windi": "npm run watch:windi"
  },
  "author": "Kura Bloodlust",
  "license": "EUPL-1.2",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "npm-run-all": "^4.1.5",
    "windicss": "^3.5.6"
  }
}